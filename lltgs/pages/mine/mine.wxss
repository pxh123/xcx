/* pages/my/my.wxss */
page{height:100%;}
.container{height:100%;background:#eee;}
.user_inf_cont{height:30vw;position:relative;}
.user_inf_cont .user_inf_bg{width:100%;height:30vw;}
.user_inf_cont .user_inf{width:80vw;height:16vw;position:absolute;left:10vw;top:7vw;}
.user_inf_cont .user_inf image{width:16vw;height:16vw;border-radius:50%;margin-right:10vw;}
.user_inf_cont .user_inf text{font-size:40rpx;line-height:16vw;display:inline-block;color:#f6f6f6;vertical-align:top}
.main{background:#fff;margin-top:20rpx;}
.main_item{font-size:32rpx;line-height:80rpx;color:#4a4a4a;position:relative;margin-left:20rpx;padding-left:20rpx;border-bottom:1px solid #eee;}
.main_item .arrow_r{position:absolute;right:20rpx;top:28rpx;width:16rpx;height:25rpx;}